{
    "name": "wasm-test",
    "description": "WASM test",
    "authors": "Roland Csaszar",
    "version": "0.1.0",
    "main": "src/main.ts",
    "devDependencies": {
        "@types/chai": "^4.3.5",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.3.1",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "chai": "^4.3.7",
        "del-cli": "^5.0.0",
        "esbuild": "^0.18.6",
        "eslint": "^8.43.0",
        "eslint-plugin-chai-friendly": "^0.7.2",
        "eslint-plugin-jsdoc": "^46.2.6",
        "eslint-plugin-mocha": "^10.1.0",
        "fast-check": "^3.10.0",
        "glob": "^10.3.0",
        "mkdirp": "^3.0.1",
        "mocha": "^10.2.0",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "scripts": {
        "clean": "del-cli -- out",
        "build": "tsc && mkdirp out && wat2wasm --enable-all ./src/test.wat -o out/test.wasm",
        "optimize": "wasm-opt ./src/test.wat --name-types --all-features -O4 -o out/test.wasm",
        "test": "mocha",
        "run-wasm": "node ./out/src/main.js"
    }
}
